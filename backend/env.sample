# Copy these keys into .env for local development
NODE_ENV=development
PORT=5000
BACKEND_API_URL=http://localhost:5000
FRONTEND_URL=http://localhost:3000
ADMIN_URL=http://localhost:3001

AZURE_STORAGE_ACCOUNT_NAME=your-storage-account
AZURE_STORAGE_KEY=your-storage-key
AZURE_STORAGE_CONNECTION_STRING=your-connection-string

MONGODB_CONNECTION_STRING=your-mongodb-uri

JWT_SECRET=your-local-jwt-secret
SESSION_SECRET=your-session-secret

# Serper.dev API key for web search functionality
SERPER_API_KEY=your-serper-api-key

# Jina AI Embedding API configuration
# URL defaults to https://api.jina.ai/v1 if not specified
EMBEDDING_API_URL=https://api.jina.ai/v1
EMBEDDING_API_KEY=your-jina-ai-api-key

ENABLE_TEST_USERS=true
OPENROUTER_REFERRER=https://localhost:3000
OPENROUTER_APP_TITLE=ChatZone Admin
